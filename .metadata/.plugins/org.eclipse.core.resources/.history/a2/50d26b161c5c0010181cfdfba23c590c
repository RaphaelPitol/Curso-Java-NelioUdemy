package main;

import java.util.Iterator;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		int[][] mat = new int[t][t];
		
		for(int i = 0; i < t; i++) {
			for(int j = 0; j < t; j++) {
				mat[i][j] = sc.nextInt();
			}
		}
		
		sc.close();
	}

}
